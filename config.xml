<?xml version='1.0' encoding='utf-8'?>
<widget id="com.terptracker.needterp" version="1.0.0" versionCode="100" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Need Terp</name>
    <description>
        Find a sign-language interpreter fast!
    </description>
    <author email="info@angelvri.com" href="http://angelvri.com/">
        Angel Interpreting
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="permissions" value="none" />
    <icon src="icon.png" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/icon-android-ldpi.png" />
        <icon density="mdpi" src="www/res/icon/android/icon-android-mdpi.png" />
        <icon density="hdpi" src="www/res/icon/android/icon-android-hdpi.png" />
        <icon density="xhdpi" src="www/res/icon/android/icon-android-xhdpi.png" />
        <icon density="xxhdpi" src="www/res/icon/android/icon-android-xxhdpi.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/icon-android-xxxhdpi.png" />
    </platform>
    <plugin name="cordova-plugin-email-composer" spec="^0.8.7" />
</widget>
